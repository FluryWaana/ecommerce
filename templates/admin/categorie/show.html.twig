{% extends 'admin/base.html.twig' %}

{% block title %}Liste des categories{% endblock %}

{% block body %}
    <h1>Liste des categories</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ article_categorie.articleCategorieId }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ article_categorie.articleCategorieNom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('categorie_index') }}">Retour</a>

    <a href="{{ path('categorie_edit', {'categorie_id': article_categorie.article_categorie_id}) }}">Modifier</a>

    {{ include('categorie/_delete_form.html.twig') }}
{% endblock %}
